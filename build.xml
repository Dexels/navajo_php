<?xml version="1.0" encoding="UTF-8" ?>
<!-- $Id$ -->
<project name="NavajoAuthPlugin" default="all" basedir=".">
	<property name="archive" value="${ant.project.name}.jar" />
	<target name="clean">
		<delete file="navajo_authentication.zip" />
		<delete file="navajo_content.zip" />
	</target>
	<target name="authentication">
		<zip zipfile="navajo_authentication.zip">
			<fileset dir="authentication" />
		</zip>
	</target>
	<target name="content">
		<zip zipfile="navajo_content.zip">
			<fileset dir="content" />
		</zip>
	</target>
	<target name="all" depends="authentication,content" />
</project>
<!-- EOF: $RCSfile$ -->
